          <Label alignment="CENTER" font="$x1" layoutX="14.0" layoutY="7.0" style="&#10;" text="View" textAlignment="CENTER" textFill="#9f9f9f" />
          <Button layoutX="244.0" layoutY="280.0" mnemonicParsing="false" onAction="#onHelloButtonClick" text="Hello!" />
               <ColorPicker layoutX="119.0" layoutY="109.0" />
        </AnchorPane>
      </ScrollPane>
    </VBox>
    <AnchorPane nodeOrientation="LEFT_TO_RIGHT" prefHeight="541.0" prefWidth="285.0">
      <Label alignment="CENTER" font="$x1" layoutX="14.0" layoutY="14.0" style="&#10;" text="Details" textAlignment="CENTER" textFill="#9f9f9f" />
         <VBox alignment="CENTER" layoutX="100.0" layoutY="60.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="401.0" prefWidth="268.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="60.0">
            <children>
          <Label fx:id="welcomeText" text="Label" />
            </children>
         </VBox>
         <MenuButton fx:id="userOptions" contentDisplay="GRAPHIC_ONLY" layoutY="-1.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" prefHeight="0.0" text="awewew" AnchorPane.rightAnchor="76.0">
           <items>
               <CustomMenuItem mnemonicParsing="false">
                  <content>
                     <Hyperlink onAction="#signInAlert" text="Sign In" textFill="#249cd3">
                        <font>
                           <Font size="14.0" />
                        </font>
                        <padding>
                           <Insets left="5.0" right="80.0" />
                        </padding>
                     </Hyperlink>
                  </content>
               </CustomMenuItem>
               <CustomMenuItem mnemonicParsing="false">
                  <content>
                     <Hyperlink alignment="CENTER" stylesheets="@main.css" text="Sign Out" textFill="#ff2727">
                        <font>
                           <Font size="14.0" />
                        </font>
                        <padding>
                           <Insets left="5.0" right="80.0" />
                        </padding>
                     </Hyperlink>
                  </content>
               </CustomMenuItem>
           </items>
         </MenuButton>
    </AnchorPane>
  </SplitPane>
  <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
    <padding>
      <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
    </padding>
    <Label maxHeight="1.7976931348623157E308" text="Left status" HBox.hgrow="ALWAYS">
      <font>
        <Font size="11.0" fx:id="x3" />
      </font>
      <textFill>
        <Color red="0.625" green="0.625" blue="0.625" fx:id="x4" />
      </textFill>
    </Label>
    <Pane HBox.hgrow="ALWAYS" />
    <Label font="$x3" text="Right status" textFill="$x4" HBox.hgrow="NEVER" />
  </HBox>
</VBox>
