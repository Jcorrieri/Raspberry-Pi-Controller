<VBox prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.app.Controller">
   <HBox id="top-hbox" alignment="CENTER_RIGHT" prefHeight="50.0" stylesheets="@main.css">
      <children>
         <Label maxWidth="1.7976931348623157E308" minWidth="-Infinity" text="Raspberry Pi Controller">
            <font>
               <Font name="Arial Rounded MT Bold" size="18.0" />
            </font>
            <padding>
               <Insets left="5.0" />
            </padding>
         </Label>
         <Pane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
         <ButtonBar buttonMinWidth="40.0" prefHeight="50.0" stylesheets="@main.css">
           <buttons>
               <Label id="display-username-buttonbar" fx:id="displayName" alignment="CENTER_RIGHT" focusTraversable="false" maxWidth="500.0" prefHeight="17.0" prefWidth="670.0" stylesheets="@main.css" text="Guest User" textAlignment="RIGHT" />
               <Button id="profile-icon" mnemonicParsing="false" onAction="#accountOptions" stylesheets="@main.css" />
           </buttons>
            <padding>
               <Insets right="15.0" />
            </padding>
         </ButtonBar>
      </children>
      <padding>
         <Insets left="10.0" />
      </padding>
   </HBox>
  <SplitPane id="main-split-pane" dividerPositions="0.2505567928730512, 0.7505567928730512" stylesheets="@main.css" VBox.vgrow="ALWAYS">
