                  <Tooltip text="Add Raspberry Pi" />
               </tooltip>
        </Button>
      </ToolBar>
      <ScrollPane id="main-scroll-pane" fitToHeight="true" fitToWidth="true" prefHeight="481.0" prefWidth="546.0" stylesheets="@main.css" VBox.vgrow="ALWAYS">
